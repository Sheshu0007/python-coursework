#1st 
year, month, date = input("Enter a date (YYYY-MM-DD): ").split('-')
print(f"{date}/{month}/{year}")



#3rd
s=input().lower()
print(s.translate(str.maketrans("aeiou","*****")))

#4th
l=list(map(float,input().split()))
print(sum(l))
print(max(l))

#6th
names=set(input().split(','))
print(sorted(names))

#8th
sen=input().split()
for i in sen:
    print(i[::-1],end=' ')

#9th
l=list(map(int,input().split()))
while (0 in l):
    l.remove(0)
print(l)


#10th
line=input()
data={}
for i in line:
    if i not in data and i!=0:
        data[i]=line.count(i)
print(data)  